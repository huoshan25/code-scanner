{
  "name": "@huo-shan/code-scanner",
  "version": "0.1.0",
  "description": "A code comment scanner tool",
  "main": "index.js",
  "types": "index.d.ts",
  "napi": {
    "name": "code-scanner",
    "triples": {
      "defaults": true,
      "additional": [
        "x86_64-pc-windows-msvc",
        "x86_64-apple-darwin",
        "x86_64-unknown-linux-gnu"
      ]
    }
  },
  "license": "MIT",
  "dependencies": {
    "@node-rs/helper": "^1.2.1"
  },
  "devDependencies": {
    "@napi-rs/cli": "^2.0.0"
  },
  "engines": {
    "node": ">= 10"
  },
  "scripts": {
    "artifacts": "napi artifacts",
    "build": "napi build --platform --release",
    "prepublishOnly": "napi prepublish -t npm"
  },
  "publishConfig": {
    "access": "public"
  },
  "optionalDependencies": {
    "@huo-shan/code-scanner-win32-x64-msvc": "0.1.0",
    "@huo-shan/code-scanner-darwin-x64": "0.1.0",
    "@huo-shan/code-scanner-linux-x64-gnu": "0.1.0"
  }
}